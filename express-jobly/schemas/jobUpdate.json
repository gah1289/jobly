{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "http://example.com/example.json",
	"type": "object",

	"required": [
		"title",
		"salary",
		"equity",
		"companyHandle"
	],
	"properties": {
		"title": {
			"type": "string",
			"minLength": 1,
			"maxLength": 30
		},
		"salary": {
			"type": "integer",
			"default": 0
		},
		"equity": {
			"type": [
				"number",
				"null"
			],
			"default": 0
		},
		"companyHandle": {
			"type": "string",
			"minLength": 1,
			"maxLength": 30
		}
	},
	"additionalProperties": false
}
